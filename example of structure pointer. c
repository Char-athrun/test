#include<stdio.h>
#include<string.h>

int main () 
{
    typedef struct player
    {
        char name[15];
        float score;
    }p;

    p aPlayer={0,0};
    p *ptrPlayer;
    ptrPlayer=&aPlayer;

    strcpy(ptrPlayer->name,"pinball Wizard");
    ptrPlayer->score=1000000.00;

    print("\nPlayer: %s\n",ptrPlayer->name);
    print("Score: %.0f\n",ptrPlayer->score);
    return 0;
}
